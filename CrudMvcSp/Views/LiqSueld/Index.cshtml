             lastText: "Ultima Pagina",
             displayHeader: true,
             columns:
              new[] {
                        grid.Column("Rut_Empleado","Rut"),
                        grid.Column("Nombre","Nombre"),
                        grid.Column("Descr_Tipo","Tipo Contrato"),
                        grid.Column("Fecha","Fecha Liquidación"),
